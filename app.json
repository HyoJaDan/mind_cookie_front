{
  "expo": {
    "name": "mindCookie",
    "slug": "mindCookie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/main.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon/main.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mindCookie",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "43.202.105.187": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/main.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.mindCookie"
    },
    "web": {
      "favicon": "./assets/icon/main.jpg"
    },
    "expo": {
      "userInterfaceStyle": "automatic"
    },
    "extra": {
      "eas": {
        "projectId": "85f5d302-89d3-499f-8de6-cd4cfd5f6157"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "supportsTablet": true,
            "infoPlist": {
              "NSAppTransportSecurity": {
                "NSAllowsArbitraryLoads": true
              }
            }
          },
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
